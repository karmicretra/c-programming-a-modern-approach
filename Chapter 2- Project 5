#include <stdio.h>
#include <math.h>
//Chapter 2. Project 5 

void main()
{
    float x;
    printf("Enter a value of x to calculate the value of the polynomial: ");
    scanf("%f", &x);
    float value = (3 * pow(x, 5)) + (2 * (pow(x, 4)) - (5 * pow(x, 3)) - pow(x, 2) + (7 * x) - 6);
    printf("The value of the polynomial is: %.4f ", value);
}
